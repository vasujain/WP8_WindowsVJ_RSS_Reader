<?xml version="1.0" encoding="utf-8"?>
<Deployment xmlns="http://schemas.microsoft.com/windowsphone/2012/deployment" AppPlatformVersion="8.0">
  <DefaultLanguage xmlns="" code="en-US" />
  <App xmlns="" ProductID="{4d18ab18-5d4a-465d-a7db-37d48f59438f}" Title="WindowsVJ Feeds" RuntimeType="Silverlight" Version="1.0.0.0" Genre="apps.normal" Author="" Description="WindowsVJ blog feed app for Windows Phone 8 grabs the RSS feed of technology blog WindowsVJ.com and displays the most recent 20 articles in tiles format. Readers can continue reading these articles within app to the blog post." Publisher="" PublisherID="{e7d94c95-4681-45a0-a8b4-5df4e482fb9e}">
    <IconPath IsRelative="true" IsResource="false">Images\Logo-4ef38213-beeb-4b1f-9ea1-5a30d80ef3d6.png</IconPath>
    <Capabilities>
      <Capability Name="ID_CAP_NETWORKING"/>
      <Capability Name="ID_CAP_MEDIALIB_AUDIO"/>
      <Capability Name="ID_CAP_MEDIALIB_PLAYBACK"/>
      <Capability Name="ID_CAP_SENSORS"/>
      <Capability Name="ID_CAP_WEBBROWSERCOMPONENT"/>
	  <Capability Name="ID_CAP_SPEECH_RECOGNITION" />
    </Capabilities>
    <Tasks>
      <DefaultTask Name="_default" NavigationPage="View/LatestPosts_News.xaml" />
    </Tasks>
    <Tokens>
      <PrimaryToken TokenID="MyDoWapp" TaskName="_default">
	    <TemplateFlip>
          <SmallImageURI IsRelative="true" IsResource="false">Images/Logo-4ef38213-beeb-4b1f-9ea1-5a30d80ef3d6.png</SmallImageURI>
		  <Count>0</Count>
		  <BackgroundImageURI IsRelative="true" IsResource="false">Images/Tile-97a06f70-0bf3-4768-b4b1-b8ea0cb957aa.png</BackgroundImageURI>
          <Title>WindowsVJ Feeds</Title>
          <BackContent></BackContent>
	      <BackBackgroundImageURI IsRelative="true" IsResource="false">Images/Tileback-46014a9d-bbd8-4e36-b7dd-e12794c8cfe7.png</BackBackgroundImageURI>
          <BackTitle></BackTitle>
		  		  <LargeBackgroundImageURI IsRelative="true" IsResource="false">Images/Tile-97a06f70-0bf3-4768-b4b1-b8ea0cb957aa.png</LargeBackgroundImageURI>
		  <LargeBackBackgroundImageURI IsRelative="true" IsResource="false">Images/Tileback-46014a9d-bbd8-4e36-b7dd-e12794c8cfe7.png</LargeBackBackgroundImageURI>
 
          <DeviceLockImageURI IsRelative="true" IsResource="false"></DeviceLockImageURI>
          <HasLarge>True</HasLarge>		
        </TemplateFlip>
      </PrimaryToken>
    </Tokens>
    <Extensions>
      <Extension ExtensionName="LockScreen_Notification_IconCount" ConsumerID="{111DFF24-AA15-4A96-8006-2BFF8122084F}" TaskID="_default" />
      <Extension ExtensionName="LockScreen_Notification_TextField" ConsumerID="{111DFF24-AA15-4A96-8006-2BFF8122084F}" TaskID="_default" />
      <Extension ExtensionName="LockScreen_Background" ConsumerID="{111DFF24-AA15-4A96-8006-2BFF8122084F}" TaskID="_default" />
    </Extensions>
    <ScreenResolutions>
      <ScreenResolution Name="ID_RESOLUTION_WVGA" />
      <ScreenResolution Name="ID_RESOLUTION_WXGA" />
      <ScreenResolution Name="ID_RESOLUTION_HD720P" />
    </ScreenResolutions>
  </App>
</Deployment>
